syntax on
filetype on
set nu

set tabstop=3 
set shiftwidth=3 
set expandtab
set smarttab

autocmd FileType make setlocal noexpandtab

set ai "auto indent
set si "smart indent
set wrap "wrap lines

set hlsearch "highlight search things

inoremap jk     <ESC>

colorscheme molokai
set background=dark
